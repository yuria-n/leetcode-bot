{
  "name": "leetcode-bot",
  "version": "0.0.0",
  "description": "Slack bot posting a LeetCode algorithm question every weekday.",
  "main": "index.ts",
  "author": "Yuria Nishimura",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^13.13.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
